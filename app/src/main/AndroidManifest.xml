<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET"/>

    <uses-permission android:name="android.permission.VIBRATE"/>

    <uses-permission android:name="android.permission.CAMERA"/>

    <uses-feature android:name="android.hardware.camera"/>

    <uses-permission android:name="android.permission.FLASHLIGHT"/>

    <uses-feature android:name="android.hardware.camera.flash"/>

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.Prueba_2"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"

        tools:targetApi="31">
        <activity
            android:name=".pantalla_carga"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" android:theme="@style/Theme.Prueba_2"
            android:exported="true"></activity>
        <activity android:name=".reproductor_mp3" android:exported="true"></activity>
        <activity android:name=".reproductor" android:exported="true"></activity>
        <activity android:name=".tienda" android:exported="true"></activity>
        <activity android:name=".calculadora_spinner" android:exported="true"></activity>
        <activity android:name=".webview" android:exported="true"></activity>
        <activity android:name=".calculadora_imc" android:exported="true"></activity>
        <activity android:name=".sueldo" android:exported="true"></activity>
        <activity android:name=".linterna" android:exported="true"></activity>
        <activity android:name=".reproductor_video" android:exported="true"></activity>
    </application>

</manifest>